{
    "include": ["./src/**/*",
      "./gatsby-node.ts"
    ],
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": ["dom", "es2019"],
      // "allowJs": true,
      // "checkJs": true,
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noEmit": true,
      "skipLibCheck": true
    }
  }
